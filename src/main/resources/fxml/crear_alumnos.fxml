<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="175.0" prefWidth="470.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <FlowPane>
         <children>
            <TextField fx:id="txtDNI" prefHeight="24.0" prefWidth="146.0" promptText="DNI">
               <FlowPane.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </FlowPane.margin>
            </TextField>
         </children>
      </FlowPane>
      <FlowPane prefHeight="34.0" prefWidth="450.0">
         <children>
            <TextField fx:id="txtNome" promptText="Nome">
               <FlowPane.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </FlowPane.margin>
            </TextField>
            <TextField fx:id="txtApelidos" prefHeight="24.0" prefWidth="256.0" promptText="Apelidos">
               <FlowPane.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </FlowPane.margin>
            </TextField>
         </children>
      </FlowPane>
      <FlowPane prefHeight="200.0" prefWidth="200.0">
         <children>
            <DatePicker fx:id="data" promptText="Data de nacemento">
               <FlowPane.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </FlowPane.margin>
            </DatePicker>
         </children>
      </FlowPane>
      <FlowPane>
         <children>
            <Button fx:id="btnAlta" alignment="TOP_LEFT" mnemonicParsing="false" onAction="#darAlta" text="Dar de alta">
               <FlowPane.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </FlowPane.margin>
            </Button>
         </children>
      </FlowPane>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
